# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true

[*]
end_of_line = lf
charset=utf-8
insert_final_newline = true

[*.{kt,kts}]
indent_style=space
indent_size=4
continuation_indent_size=4
max_line_length=off

[*.md]
trim_trailing_whitespace=false
max_line_length=off
